{
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ],
    "strictNullChecks": true
  },
  "include": [
    "next-env.d.ts",
    ".next/types/**/*.ts",
    "**/*.ts",
    "**/*.tsx",
    "app/_data/serviceData.ts",
    "app/_data/reviewGuest.ts",
    "app/_data/postCategoryItem.ts",
    "app/_data/navItem.ts",
    "app/_data/locationdata.ts",
    "app/_data/getListingData.ts",
    "app/_data/getBlogPostItem.ts",
    "app/_data/listingCategoryItem.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
